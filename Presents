#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

#define ryu ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);

int main()
{
	ryu
	int n;
	cin>>n;
	int m[105];
	int i,j;
	for(i=0; i<n; i++)
	{
		cin>>m[i];
	}
	for(i=1; i<=n; i++)
	{
		for(j=0; j<n; j++)
		{
			if(m[j]==i)
				cout<<j+1<<" ";
		}
	}
	return 0;
} 
