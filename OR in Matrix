#include <iostream>
#include <vector>
using namespace std;

int main() {
    int m, n;
    cin >> m >> n;

    // Input matrix B
    vector<vector<int>> B(m, vector<int>(n));
    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            cin >> B[i][j];
        }
    }

    // Initialize matrix A with 1s
    vector<vector<int>> A(m, vector<int>(n, 1));

    // Apply constraints: if B[i][j] == 0, set the corresponding row and column in A to 0
    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            if (B[i][j] == 0) {
                for (int k = 0; k < n; ++k) A[i][k] = 0; // Set row i to 0
                for (int k = 0; k < m; ++k) A[k][j] = 0; // Set column j to 0
            }
        }
    }

    // Verify if the constructed A produces the given B
    bool valid = true;
    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            int row_or = 0, col_or = 0;
            for (int k = 0; k < n; ++k) row_or |= A[i][k]; // OR all elements in row i
            for (int k = 0; k < m; ++k) col_or |= A[k][j]; // OR all elements in column j
            if ((row_or | col_or) != B[i][j]) { // Verify B[i][j]
                valid = false;
                break;
            }
        }
        if (!valid) break;
    }

    // Output result
    if (valid) {
        cout << "YES\n";
        for (int i = 0; i < m; ++i) {
            for (int j = 0; j < n; ++j) {
                cout << A[i][j] << " ";
            }
            cout << "\n";
        }
    } else {
        cout << "NO\n";
    }

    return 0;
}
