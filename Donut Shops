#include <bits/stdc++.h>
using namespace std;

void solve() {
    int t;
    cin >> t;
    while (t--) {
        long long a, b, c;
        cin >> a >> b >> c;
        
        long long x1 = (a < (c + 0LL)) ? 1 : -1;
        
        long long x2 = (b * a > c) ? b : -1;
        
        cout << x1 << " " << x2 << endl;
    }
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    
    solve();
    return 0;
}
